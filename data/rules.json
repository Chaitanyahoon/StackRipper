{
  "technologies": [
    {
      "name": "HTML5",
      "category": "Core Web",
      "description": "Standard markup language for modern web documents.",
      "website": "https://developer.mozilla.org/en-US/docs/Web/HTML",
      "patterns": {
        "dom": [
          "html"
        ],
        "scripts": [],
        "headers": []
      }
    },
    {
      "name": "JavaScript",
      "category": "Core Web",
      "description": "The engine of the interactive web.",
      "website": "https://developer.mozilla.org/en-US/docs/Web/JavaScript",
      "patterns": {
        "dom": [
          "script"
        ],
        "scripts": [],
        "headers": []
      }
    },
    {
      "name": "React",
      "category": "Frontend",
      "description": "Component-based UI library by Meta.",
      "website": "https://react.dev",
      "patterns": {
        "dom": [
          "#root",
          "#app",
          "[data-reactroot]"
        ],
        "scripts": [
          "react-dom",
          "react\\.production",
          "fkvendor\\.js"
        ],
        "headers": []
      }
    },
    {
      "name": "Next.js",
      "category": "Frontend",
      "description": "React framework for production-grade web apps.",
      "website": "https://nextjs.org",
      "patterns": {
        "dom": [
          "#__next",
          "script[src*='_next/static']"
        ],
        "scripts": [
          "/_next/static/"
        ],
        "headers": [
          "X-Nextjs-Cache",
          "X-Vercel-Cache"
        ]
      }
    },
    {
      "name": "Vue.js",
      "category": "Frontend",
      "description": "Progressive JavaScript framework for building interfaces.",
      "website": "https://vuejs.org",
      "patterns": {
        "dom": [
          "[data-v-]",
          "#app"
        ],
        "scripts": [
          "vue\\.js",
          "vue\\.min\\.js",
          "vue@"
        ],
        "headers": []
      }
    },
    {
      "name": "Angular",
      "category": "Frontend",
      "description": "Platform for building mobile and desktop web apps by Google.",
      "website": "https://angular.io",
      "patterns": {
        "dom": [
          "[ng-version]",
          "app-root",
          "script[id*='angular']"
        ],
        "scripts": [
          "angular\\.js",
          "zone\\.js",
          "@angular"
        ],
        "headers": []
      }
    },
    {
      "name": "Svelte",
      "category": "Frontend",
      "description": "Cybernetically enhanced web apps with no virtual DOM.",
      "website": "https://svelte.dev",
      "patterns": {
        "dom": [
          ".svelte-"
        ],
        "scripts": [
          "svelte"
        ],
        "headers": []
      }
    },
    {
      "name": "Vite",
      "category": "Build Tools",
      "description": "The next generation frontend tooling.",
      "website": "https://vitejs.dev",
      "patterns": {
        "dom": [
          "script[type='module'][src*='@vite']"
        ],
        "scripts": [
          "/@vite/",
          "vite\\.svg"
        ],
        "headers": []
      }
    },
    {
      "name": "Flipkart Stack",
      "category": "E-commerce",
      "description": "Custom high-performance React-based architecture.",
      "website": "https://flipkart.com",
      "patterns": {
        "dom": [
          "link[href*='flixcart.com']",
          "#container"
        ],
        "scripts": [
          "flixcart\\.com",
          "flixcart\\.net"
        ],
        "headers": []
      }
    },
    {
      "name": "Tailwind CSS",
      "category": "UI Libraries",
      "description": "Utility-first CSS framework for modern design.",
      "website": "https://tailwindcss.com",
      "patterns": {
        "dom": [
          ".sm\\:",
          ".md\\:",
          ".lg\\:",
          ".xl\\:",
          ".\\["
        ],
        "scripts": [],
        "headers": []
      }
    },
    {
      "name": "Bootstrap",
      "category": "UI Libraries",
      "description": "The most popular CSS framework for responsive design.",
      "website": "https://getbootstrap.com",
      "patterns": {
        "dom": [
          "link[href*='bootstrap']",
          ".container-fluid"
        ],
        "scripts": [
          "bootstrap\\.min\\.js"
        ],
        "headers": []
      }
    },
    {
      "name": "Cloudflare",
      "category": "Hosting / CDN",
      "description": "Global performance and security network.",
      "website": "https://cloudflare.com",
      "patterns": {
        "dom": [],
        "scripts": [],
        "headers": [
          "cf-ray",
          "cf-cache-status",
          "server: cloudflare"
        ]
      }
    },
    {
      "name": "Vercel",
      "category": "Hosting / CDN",
      "description": "Global CDN and cloud platform for frontend frameworks.",
      "website": "https://vercel.com",
      "patterns": {
        "dom": [],
        "scripts": [],
        "headers": [
          "x-vercel-id",
          "x-vercel-cache"
        ]
      }
    },
    {
      "name": ".NET",
      "category": "Backend",
      "description": "Cross-platform developer platform by Microsoft.",
      "website": "https://dotnet.microsoft.com",
      "patterns": {
        "dom": [
          "__VIEWSTATE"
        ],
        "scripts": [],
        "headers": [
          "x-powered-by: ASP.NET",
          "server: Microsoft-IIS"
        ]
      }
    },
    {
      "name": "Node.js",
      "category": "Backend",
      "description": "JavaScript runtime built on Chrome's V8 engine.",
      "website": "https://nodejs.org",
      "patterns": {
        "dom": [],
        "scripts": [],
        "headers": [
          "x-powered-by: express",
          "x-powered-by: nodejs"
        ]
      }
    },
    {
      "name": "WordPress",
      "category": "CMS",
      "description": "World's most popular open-source CMS.",
      "website": "https://wordpress.org",
      "patterns": {
        "dom": [
          "link[href*='wp-content']",
          "meta[content*='WordPress']"
        ],
        "scripts": [
          "wp-embed\\.min\\.js"
        ],
        "headers": [
          "X-Pingback"
        ]
      }
    },
    {
      "name": "New Relic",
      "category": "Analytics",
      "description": "Enterprise observability and performance monitoring.",
      "website": "https://newrelic.com",
      "patterns": {
        "dom": [],
        "scripts": [
          "newrelic",
          "nr-runtime"
        ],
        "headers": [
          "x-newrelic-id"
        ]
      }
    },
    {
      "name": "Google Analytics",
      "category": "Analytics",
      "description": "Web traffic tracking and reporting service.",
      "website": "https://analytics.google.com",
      "patterns": {
        "dom": [],
        "scripts": [
          "gtag/js",
          "analytics\\.js"
        ],
        "headers": []
      }
    }
  ]
}